@startuml
actor HR as "HR Advisor"

actor employee as "Employee"

rectangle "HR System" {


    usecase UC5 as "Employee Details File"

    usecase UC4 as "New employee details"

    usecase UCa as "Update employee details"

    HR - UC5
        UC5 ..> UCa : include
        UC4 - employee
        UC5 - UC4
@enduml